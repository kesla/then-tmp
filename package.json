{
  "name": "then-tmp",
  "version": "0.0.0",
  "description": "Promised version of the node.js tmp module.",
  "main": "dist/index.js",
  "scripts": {
    "test": "xo && babel-tape-runner test.js",
    "build": "rm -rf dist && mkdir -p dist && babel lib --out-dir dist",
    "watch": "rm -rf dist && mkdir -p dist && babel -w lib --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kesla/then-tmp.git"
  },
  "author": "David Bj√∂rklund <david.bjorklund@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kesla/then-tmp/issues"
  },
  "homepage": "https://github.com/kesla/then-tmp#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015-node4": "2.1.0",
    "babel-tape-runner": "^2.0.1",
    "tapava": "^2.2.0",
    "xo": "^0.16.0"
  },
  "xo": {
    "esnext": true,
    "space": true
  }
}
